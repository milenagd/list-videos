{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "src": "./src",
          "components": "./src/components",
          "constants": "./src/constants",
          "icons": "./src/icons",
          "styles": "./src/styles",
          "integrations": "./src/integrations",
          "services": "./src/services"
        }
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
